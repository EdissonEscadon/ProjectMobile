<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetalleUniforme">

    <TextView
        android:id="@+id/txtNomEscuela"
        android:layout_width="295dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:text='Unidad Educativa "Tecnico Salesiano"'
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.594"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtdetalleuni"
        android:layout_width="159dp"
        android:layout_height="33dp"
        android:layout_marginTop="16dp"
        android:text="Uniforme de Gala"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNomEscuela" />
</androidx.constraintlayout.widget.ConstraintLayout>